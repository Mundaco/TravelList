<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.travellist"
    android:versionCode="1"
    android:versionName="1.0">

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        android:allowBackup="false"
        android:supportsRtl="true">
        <activity
            android:name=".TravelListActivity"
            android:label="@string/title_activity_travel_list"
            android:launchMode="singleTop"
            android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TravelActivity"
            android:parentActivityName=".TravelListActivity"
            android:screenOrientation="sensor">
        </activity>
        <activity
            android:name=".TravelEditActivity"
            android:parentActivityName=".TravelListActivity"
            android:screenOrientation="sensor">
        </activity>
    </application>

</manifest>